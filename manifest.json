{
  "manifest_version": 3,
  "name": "FillJob",
  "version": "1.0",
  "description": "This extension autofills forms lightning fast.",
  "icons": {
    "48": "filljob-logo.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
